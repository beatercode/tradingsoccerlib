{
  "name": "tradingsoccerlib",
  "version": "0.1.2",
  "description": "tradingsoccerlib",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/umd/tradingsoccerlib.min.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "LICENSE",
    "README.md",
    "licenses/",
    "NOTICE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/beatercode/tradingsoccerlib"
  },
  "bugs": {
    "url": "https://github.com/beatercode/tradingsoccerlib/issues"
  },
  "homepage": "https://github.com/beatercode/tradingsoccerlib",
  "keywords": [
    "time-line",
    "candlestick",
    "indicator",
    "finance",
    "stockchart",
    "stock",
    "chart",
    "canvas"
  ],
  "author": "beatercode",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "node scripts/clean.js",
    "build-esm": "node scripts/build-esm.js",
    "build-cjs": "cross-env NODE_ENV=production node scripts/build-cjs.js",
    "build-umd:dev": "cross-env NODE_ENV=development node scripts/build-umd.js",
    "build-umd:prod": "cross-env NODE_ENV=production node scripts/build-umd.js",
    "build-umd": "npm run build-umd:dev && npm run build-umd:prod",
    "build-core": "npm run build-esm && npm run build-cjs && npm run build-umd",
    "build-dts": "dts-bundle-generator --no-banner true --fail-on-class true --umd-module-name tradingsoccerlib -o dist/index.d.ts src/index.ts",
    "build": "npm run clean && npm run build-core && npm run build-dts",
    "dist:build": "npm run build && npm version patch --force && git add . && git commit -m 'bump version' && git push",
    "dist": "npm version patch --force && git add . && git commit -m 'bump version' && git push",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "@babel/standalone": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@stackblitz/sdk": "^1.9.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chalk": "^5.3.0",
    "codesandbox": "^2.2.3",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^9.0.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^4.7.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "tslib": "^2.5.0",
    "typescript": "^4.8.3",
    "vitepress": "1.0.0-rc.45"
  }
}
